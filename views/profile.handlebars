 <div class="container">
   


  <div id="profile-container">

    <div id="profile-image">
      <img src="{{info.imageURL}}" class="profile-pic" class="img-rounded">
    </div>
<br>

	<ul class="nav nav-tabs">
  <li class="active"><a href="#contact" data-toggle="tab">Contact</a></li>
  <li><a href="#enrolledClasses" data-toggle="tab">Classes</a></li>
  <li><a href="#classHistory" data-toggle="tab">Mutual</a></li>
  </ul>
	<div class="tab-content">

    <div class="tab-pane active" id="contact">
      <h4><span class="glyphicon glyphicon-user"></span> Contact</h4>

      <table class="table">
        <tbody>
          <tr>
            <td>Name</td>
            <td id="profile-name">{{info.firstName}} {{info.lastName}}</td>
          </tr>
          <tr>
            <td>Email</td>
            <td id="profile-email">{{info.email}}</td>
          </tr>
        </tbody>
      </table>
    </div>
	
	

    <div class="tab-pane" id="enrolledClasses">
      <h4><span class="glyphicon glyphicon-tasks"></span> Enrolled Classes</h4>


  
    
      <table class="table table-hover classes">
        <thead>
          <th> 
             ID
          </th>
          <th> 
             Name
          </th>
          <th> 
             Section
          </th>
        </thead>
        <tbody>
          {{#each current_classes}}
          <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{section}} </td>
          </tr>
          {{/each}}
        </tbody>
      </table>


           <h4><span class="glyphicon glyphicon-time"></span> Class History</h4>

      <table class="table table-hover classes">
        <thead>
          <th> 
             ID
          </th>
          <th> 
             Name
          </th>
          <th> 
             Section
          </th>
        </thead>
        <tbody>
          {{#each previous_classes}}
          <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{section}} </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
   
     
    </div>

    <div class="tab-pane" id="classHistory">
      <h4><span class="glyphicon glyphicon-tasks"></span> Enrolled Classes</h4>

      <table class="table table-hover classes">
        <thead>
          <th> 
             ID
          </th>
          <th> 
             Name
          </th>
          <th> 
             Section
          </th>
        </thead>
        <tbody>
          {{#each current_classes_mutual}}
          <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{section}} </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
      <h4><span class="glyphicon glyphicon-time"></span> Class History</h4>

      <table class="table table-hover classes">
        <thead>
          <th> 
             ID
          </th>
          <th> 
             Name
          </th>
          <th> 
             Section
          </th>
        </thead>
        <tbody>
          {{#each previous_classes_mutual}}
          <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{section}} </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    

  </div>


