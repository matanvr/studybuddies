    <link href="css/starter-template.css" rel="stylesheet">
  </head>

<body data-spy="scroll" data-target=".navbar-example">

    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" id="home-button" href="./homepage" > Classbook</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li><a href="./homepage">My Classes</a></li>
            <li><a href="./my-profile">My Profile</a></li>
            <li><a href="./help">Help</a></li>
             <li><a href="./logout">Sign out</a></li>

          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>
 <div class="container">

  <!-- Nav Bar Class -->
    <nav id="navbar-example2" class="navbar navbar-default " role="navigation">

      <div class="container-fluid">
      
     <!-- Classes Header -->
        <div class="navbar-header">
          
        </div>
        <p class="navbar-text" id="class-title"> {{classInfo.courseName}} - {{classInfo.className}} ({{classInfo.sectionID}}) </p>
    <!-- End Classes Header -->

      </div>

    </nav>

  {{#if group}} 
  <ul class="nav nav-tabs">
    <li class="active"><a href="#classMates" data-toggle="tab">All</a></li>
      <li><a href="#groupMates" data-toggle="tab" >Groupmates</a></li>
  </ul>
<div class="tab-content">
  <div id="classMates"  class="tab-pane active"> 
        <!-- End Nav Bar Class -->
        <div class="input">
        <input id="filter" type="text" class="form-control" placeholder="Filter Students">
      </div><br>
        <!-- Classes Info Class -->
          <div data-spy="scroll" data-target="#navbar-example2" data-offset="0" class="scrollspy-example fixed-box">
          <!-- Content for classes -->

       
          <table class="table classmates table-striped">
            <thead>
              <tr <tr class="btn-primary">
                <th>Profile</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th> Group Status </th>
              </tr>
            </thead>
            <tbody>
              {{#each students}}
              <tr id='{{id}}'>
                <td class="clickable first"><img src='{{imageURL}}' height="75px" width="75px" class="img-circle"></td>
                <td class="clickable first">{{firstName}}</td>
                <td class="clickable first">{{lastName}}</td>
                <td class="invite-button">
                  {{#if clickable}}
                  <button type="button" class="btn btn-warning " data-toggle="modal" data-target="#myModal{{firstName}}{{lastName}}">Invite to Group</button> 

                    <!-- Modal -->
                    <div class="modal fade" id="myModal{{firstName}}{{lastName}}" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                      <div class="modal-dialog">
                        <div class="modal-content">
                          <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                            <h4 class="modal-title" id="myModalLabel">Groupmate Invite</h4>
                          </div>
                          <div class="modal-body">
                      
                            <div>   
                        <b>    Are you sure you want to invite {{firstName}} {{lastName}} to your group? </b>
                      </div>
                      <br>
                            <img src='{{imageURL}}' height="100px" width="100px" class="img-circle" style="text-align:center;">
                           </div>
                          <div class="modal-footer">
                            <button type="button" class="btn btn-danger" data-dismiss="modal">Cancel</button>
                            <button type="button" class="btn btn-success invite" data-dismiss="modal">Yes!</button>
                          </div>
                        </div>
                      </div>
                    </div>
                    {{/if}}
                </td>
              </tr>
              {{/each}}
            </tbody>
          </table>
           

        </div>
          <!-- End content for classes -->
    </div>
    <br><br>
    <div id="groupMates" class="tab-pane">
          <table class="table table-striped" id="groupmatestable">
            <thead>
              <tr class="btn-primary">
                <th>Profile</th>
                <th>First Name</th>
                <th>Last Name</th>
              </tr>
            </thead>
            <tbody> 
              <tr id='{{user.id}}'>
                <td class="clickable first"><img src='{{user.imageURL}}' height="75px" width="75px" class="img-circle"></td>
                <td class="clickable first">{{user.firstName}}</td>
                <td class="clickable first">{{user.lastName}}</td>
            </tbody>
          </table>

    </div> 
   </div>  
      
  </div>
  {{else}}

    <div class="input">
  <input id="filter" type="text" class="form-control" placeholder="Filter Students">
</div><br>
  <!-- Classes Info Class -->
    <div data-spy="scroll" data-target="#navbar-example2" data-offset="0" class="scrollspy-example fixed-box">
    <!-- Content for classes -->

 
    <table class="table classmates table-striped">
      <thead>
        <tr <tr class="btn-primary">
          <th>Profile</th>
          <th>First Name</th>
          <th>Last Name</th>
        </tr>
      </thead>
      <tbody>
        {{#each students}}
        <tr id='{{id}}'>
                <td class="clickable second"><img src='{{imageURL}}' height="75px" width="75px" class="img-circle"></td>
                <td class="clickable second">{{firstName}}</td>
                <td class="clickable second">{{lastName}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
     

  </div>
  {{/if}}

